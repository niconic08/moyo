def F(n):
    if n <= 13:
        return n**3+n**2+1
    elif n > 13 and n % 3 == 0:
        return F(n-1)+2*n*n-3
    elif n > 13 and n % 3 != 0:
        return F(n-2)+3*n+6


for i in range(1, 1001):
    a = list(str(F(i)))
    print(a)
    print([int(_) for _ in a if int(_) % 2 != 0])
    if a == [int(_) for _ in a if int(_) % 2 != 0]:
        print(i)
